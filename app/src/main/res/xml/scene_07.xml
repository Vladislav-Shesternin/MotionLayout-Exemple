<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">
    <Transition
        motion:constraintSetEnd="@+id/no_07"
        motion:constraintSetStart="@+id/no_07"
        motion:duration="3000">

        <KeyFrameSet>
            <KeyCycle
                android:translationX="10dp"
                motion:framePosition="0"
                motion:motionTarget="@+id/edit_07_2"
                motion:wavePeriod="5"
                motion:waveShape="sin" />

            <KeyPosition
                motion:framePosition="10"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/btn_1"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="20"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/btn_1"
                motion:percentY="0.56" />
            <KeyPosition
                motion:framePosition="35"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/btn_1"
                motion:percentY="0.45" />
            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/btn_1"
                motion:percentY="0.56" />
            <KeyPosition
                motion:framePosition="75"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/btn_1"
                motion:percentY="0.4" />

            <KeyAttribute
                motion:framePosition="5"
                motion:motionTarget="@+id/edit_07_2">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@color/colorRed_08" />
            </KeyAttribute>

            <KeyAttribute
                android:scaleX="0.9"
                android:scaleY="1.2"
                motion:framePosition="10"
                motion:motionTarget="@+id/btn_1" />

            <KeyAttribute
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="20"
                motion:motionTarget="@+id/btn_1" />
            <KeyAttribute
                android:scaleX="0.85"
                android:scaleY="1.25"
                motion:framePosition="35"
                motion:motionTarget="@+id/btn_1" />
            <KeyAttribute
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="50"
                motion:motionTarget="@+id/btn_1" />
            <KeyAttribute
                android:scaleX="0.8"
                android:scaleY="1.30"
                motion:framePosition="75"
                motion:motionTarget="@+id/btn_1" />
        </KeyFrameSet>
    </Transition>

    <Transition
        motion:constraintSetEnd="@+id/yes_07"
        motion:constraintSetStart="@+id/yes_07"
        motion:duration="3000">

        <KeyFrameSet>
            <KeyCycle
                android:translationY="10dp"
                motion:framePosition="0"
                motion:motionTarget="@+id/edit_07_2"
                motion:wavePeriod="5"
                motion:waveShape="sin" />

            <KeyPosition
                motion:framePosition="10"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/btn_1"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="20"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/btn_1"
                motion:percentY="0.56" />
            <KeyPosition
                motion:framePosition="35"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/btn_1"
                motion:percentY="0.45" />
            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/btn_1"
                motion:percentY="0.56" />
            <KeyPosition
                motion:framePosition="75"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/btn_1"
                motion:percentY="0.4" />

            <KeyAttribute
                motion:framePosition="5"
                motion:motionTarget="@+id/edit_07_2">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@color/colorAccent" />
            </KeyAttribute>

            <KeyAttribute
                android:scaleX="0.9"
                android:scaleY="1.2"
                motion:framePosition="10"
                motion:motionTarget="@+id/btn_1" />
            <KeyAttribute
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="20"
                motion:motionTarget="@+id/btn_1" />
            <KeyAttribute
                android:scaleX="0.85"
                android:scaleY="1.25"
                motion:framePosition="35"
                motion:motionTarget="@+id/btn_1" />
            <KeyAttribute
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition="50"
                motion:motionTarget="@+id/btn_1" />
            <KeyAttribute
                android:scaleX="0.8"
                android:scaleY="1.30"
                motion:framePosition="75"
                motion:motionTarget="@+id/btn_1" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/no_07">
        <Constraint android:id="@+id/edit_07_2">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                motion:layout_constraintEnd_toEndOf="@+id/edit_07_1"
                motion:layout_constraintStart_toStartOf="@+id/edit_07_1"
                motion:layout_constraintTop_toBottomOf="@+id/edit_07_1" />
            <CustomAttribute
                motion:attributeName="backgroundColor"
                motion:customColorValue="@color/colorYellow_02" />
        </Constraint>

        <Constraint android:id="@+id/btn_1">
            <Layout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="80dp"
                motion:layout_constraintEnd_toEndOf="@+id/edit_07_2"
                motion:layout_constraintStart_toStartOf="@+id/edit_07_2"
                motion:layout_constraintTop_toBottomOf="@+id/edit_07_2" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet
        android:id="@+id/yes_07"
        motion:deriveConstraintsFrom="@+id/no_07" />
</MotionScene>